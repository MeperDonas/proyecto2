<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= title %></title>
</head>
<body>
  <ul>
    <% products.forEach(function(product) { %>
      <li><%= product.nombre %></li>
      <li><%= product.precio %></li>
      <a href="/productos/eliminar/<%= product.id %>">Eliminar</a>
    <% }); %>
  </ul>

</body>
</html>
